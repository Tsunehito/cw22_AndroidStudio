<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".MainActivity" >

    <Button
        android:id="@+id/ssThread"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/sans_thread" />

    <Button
        android:id="@+id/acThread"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/avec_thread" />

    <Button
        android:id="@+id/loadData"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/loadData" />

    <ImageView
        android:id="@+id/imageData"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:srcCompat="@android:drawable/sym_def_app_icon"
        android:contentDescription="@string/descriptionImageData" />
</LinearLayout>